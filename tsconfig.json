{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["es6"],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "rootDir": "src",
    "outDir": "dist",
    "noImplicitAny": false,
    "sourceMap": false,
    "moduleResolution": "node",
    "noUnusedLocals": true
  }
}
